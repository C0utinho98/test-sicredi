';\n\ninterface InputProps extends InputHTMLAttributes<HTMLInputElement> {\n  name: string;\n  icon?: React.ComponentType<IconBaseProps>;\n  inputRef?: any;\n  error?: boolean;\n  msgError?: string;\n}\n\nconst Input: React.FC<InputProps> = ({\n  icon: Icon,\n  inputRef,\n  error,\n  msgError,\n  ...rest\n}) => {\n  const [isFocused, setIsFocused] = useState(false);\n\n  return (\n    <Container isFocused={isFocused} isError={error} data-testid=\"input-test\">\n      {Icon && <Icon size={20} />}\n      <input\n        onFocus={() => setIsFocused(true)}\n        onBlur={() => setIsFocused(false)}\n        {...rest}\n        ref={inputRef}\n      />\n      {error && (\n        <Error title={msgError}>\n          <FiAlertCircle color=\"#c53030\" size={20} />\n        </Error>\n      )}\n    </Container>\n  );\n};\n\nexport default Input;\n","/Users/emerson.coutinho/Desktop/Emerson/dragons/src/components/tooltip/index.tsx",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/components/input/styles.ts",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/components/tooltip/styles.tsx",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/components/index.ts",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/store/auth/reducer/actions.ts",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/components/header/index.tsx",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/components/header/styles.ts",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/components/container/style.ts",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/pages/notFound/styles.ts",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/components/accordion/index.tsx",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/components/accordion/styles.ts",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/components/toast/index.tsx",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/components/toast/styles.ts",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/store/toast/reducer/index.ts",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/store/toast/reducer/actions.ts",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/pages/createOrUpdate/index.tsx",["240"],"import React, { useCallback, useEffect, useState } from 'react';\nimport { useForm } from 'react-hook-form';\nimport { GiDragonHead } from 'react-icons/gi';\nimport { IoIosArrowBack } from 'react-icons/io';\nimport { GiSeaDragon, GiDragonOrb } from 'react-icons/gi';\nimport { useDispatch } from 'react-redux';\nimport { useHistory, useParams } from 'react-router-dom';\nimport { openToast } from '../../store/toast/reducer/actions';\n\nimport { Input, Button } from '../../components';\nimport api from '../../services/api';\nimport { Form, Icon, Circle, WrapperButtons } from './styles';\n\ninterface Dragon {\n  name: string;\n  type: string;\n}\n\ninterface Params {\n  id: string;\n}\n\nconst CreateOrUpdate: React.FC = () => {\n  const { id } = useParams<Params>();\n  const [loading, setLoading] = useState(false);\n  const history = useHistory();\n  const dispatch = useDispatch();\n  const { register, handleSubmit, errors, setValue } = useForm();\n  useEffect(() => {\n    if (id) {\n      api\n        .get<Dragon>(`/dragon/${id}`)\n        .then(response => {\n          const { name, type } = response.data;\n          setValue('name', name);\n          setValue('type', type);\n        })\n        .catch(() => {\n          dispatch(\n            openToast({\n              title: 'Erro',\n              message: 'Erro ao encontrar Dragão!',\n              type: 'error',\n            }),\n          );\n          history.push('/main');\n        });\n    } else {\n      setValue('name', '');\n      setValue('type', '');\n    }\n  }, [id]);\n\n  const onSubmit = handleSubmit((data: Dragon): void => {\n    setLoading(true);\n    if (id) {\n      api\n        .put(`/dragon/${id}`, data)\n        .then(() => {\n          dispatch(\n            openToast({\n              title: 'Sucesso',\n              message: 'Dragão atualizado com sucesso!',\n              type: 'success',\n            }),\n          );\n          history.push('/main');\n        })\n        .catch(() => {\n          dispatch(\n            openToast({\n              title: 'Erro',\n              message: 'Erro ao atualizar!',\n              type: 'error',\n            }),\n          );\n          setLoading(false);\n        });\n    } else {\n      api\n        .post('/dragon', data)\n        .then(() => {\n          setLoading(false);\n          dispatch(\n            openToast({\n              title: 'Sucesso',\n              message: 'Dragão cadastrado com sucesso!',\n              type: 'success',\n            }),\n          );\n          history.push('/main');\n        })\n        .catch(() => {\n          dispatch(\n            openToast({\n              title: 'Erro',\n              message: 'Erro ao cadastrar!',\n              type: 'error',\n            }),\n          );\n          setLoading(false);\n        });\n    }\n  });\n\n  const handleDelete = useCallback(\n    (idDragon: string) => {\n      api\n        .delete(`/dragon/${idDragon}`)\n        .then(() => {\n          setLoading(false);\n          dispatch(\n            openToast({\n              title: 'Excluído',\n              message: 'Dragão excluído com sucesso!',\n              type: 'success',\n            }),\n          );\n          history.push('/main');\n        })\n        .catch(() => {\n          openToast({\n            title: 'Erro',\n            message: 'Erro ao excluir Dragão!',\n            type: 'error',\n          });\n          setLoading(false);\n        });\n    },\n    [dispatch, history],\n  );\n\n  const validateSpace = (value: string) => {\n    return value.trim().length !== 0;\n  };\n\n  return (\n    <Form autoComplete=\"off\" onSubmit={onSubmit}>\n      <IoIosArrowBack\n        size={30}\n        onClick={() => history.push('/main')}\n        data-testid=\"back-button-test\"\n      />\n      <Icon>\n        <Circle error={errors.name || errors.type}>\n          <GiDragonHead size={100} color=\"#d8e1e9\" />\n        </Circle>\n      </Icon>\n      <Input\n        name=\"name\"\n        placeholder=\"Nome do Dragão\"\n        inputRef={register({\n          required: true,\n          validate: value => validateSpace(value),\n        })}\n        msgError=\"Nome do dragão é obrigatório\"\n        error={errors.name}\n        icon={GiSeaDragon}\n      />\n      <Input\n        name=\"type\"\n        placeholder=\"Tipo do Dragão\"\n        inputRef={register({\n          required: true,\n          validate: value => validateSpace(value),\n        })}\n        msgError=\"Tipo do dragão é obrigatório\"\n        error={errors.type}\n        icon={GiDragonOrb}\n      />\n      {!id ? (\n        <Button type=\"submit\" loading={loading}>\n          Cadastrar\n        </Button>\n      ) : (\n        <WrapperButtons>\n          <Button\n            type=\"button\"\n            color=\"#ff6961\"\n            disabled={loading}\n            onClick={() => handleDelete(id)}\n          >\n            Excluir\n          </Button>\n          <Button type=\"submit\" loading={loading} disabled={loading}>\n            Editar\n          </Button>\n        </WrapperButtons>\n      )}\n    </Form>\n  );\n};\n\nexport default CreateOrUpdate;\n","/Users/emerson.coutinho/Desktop/Emerson/dragons/src/pages/createOrUpdate/styles.ts",[],["241","242","243","244","245"],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/components/pagination/index.tsx",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/components/pagination/styles.ts",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/store/dragons/reducer/index.ts",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/store/dragons/saga/index.ts",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/services/api.ts",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/store/dra