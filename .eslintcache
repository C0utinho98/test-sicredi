tent } from './styles';\n\nconst Main: React.FC = () => {\n  const history = useHistory();\n  const dispatch = useDispatch();\n  const [page, setPage] = useState(0);\n\n  const dragons = useSelector<IState, [Dragon[]]>(\n    state => state.dragon.dragons,\n  );\n\n  useEffect(() => {\n    api\n      .get('/dragon')\n      .then(response => dispatch(getDragonsRequest(response.data as Dragon[])))\n      .catch(() =>\n        dispatch(\n          openToast({\n            title: 'Erro',\n            message: 'Erro ao retornar lista',\n            type: 'error',\n          }),\n        ),\n      );\n  }, [dispatch]);\n\n  const displayDays = (date: Date) => {\n    const localDate = new Date(date);\n    return format(localDate, \"'Dia' dd 'de' MMMM 'de' yyyy\", { locale: ptBr });\n  };\n\n  return (\n    <Container>\n      <div style={{ minHeight: '600px' }}>\n        {dragons[page] &&\n          dragons[page].map(el => (\n            <Accordion title={el.name} key={el.id}>\n              <Content>\n                <div style={{ display: 'flex', flexDirection: 'column' }}>\n                  <span>Data de criação:</span>\n                  <span>{`Nome: ${el.name}`}</span>\n                  <span>{`Tipo: ${el.type}`}</span>\n                </div>\n                <Button\n                  onClick={() => history.push(`/update/${el.id}`)}\n                  data-testid={el.name}\n                >\n                  Editar\n                </Button>\n              </Content>\n            </Accordion>\n          ))}\n      </div>\n      {dragons[page] && (\n        <Pagination\n          size={dragons.length}\n          page={page + 1}\n          changePage={changePage => setPage(changePage - 1)}\n        />\n      )}\n    </Container>\n  );\n};\n\nexport default Main;\n","/Users/emerson.coutinho/Desktop/Emerson/dragons/src/pages/notFound/index.tsx",[],"/Users/emerson.coutinho/Desktop/Emerson/dragons/src/components/button/index.tsx",[],"/Users/emerson.coutinho/Desktop/Eme